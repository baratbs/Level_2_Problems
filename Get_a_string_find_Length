#include<stdio.h>
int main(){

char string[50];
int length=0;

printf("Give me a string:");

scanf("%s",string);
  
length=string_length(string);

printf("The length of the string is:%d\n",length);
}

int string_length(char* string){
int l_string=0;
	while(*string!='\0'){
		l_string++;
		string++;
	}
return l_string;
}
